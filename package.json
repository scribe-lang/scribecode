{
    "name": "scribe",
    "displayName": "scribe",
    "description": "Syntax Highlighting of Scribe Programming Language for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "scribe",
            "aliases": ["Scribe", "scribe"],
            "extensions": [".sc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "scribe",
            "scopeName": "source.scr",
            "path": "./syntaxes/scribe.tmLanguage.json"
        }]
    }
}