{
  "name": "scribe",
  "displayName": "Scribe",
  "description": "Syntax Highlighting for Scribe Programming Language in VS Code",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/scribelang/scribecode.git"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "scribe",
        "aliases": [
          "Scribe",
          "scribe"
        ],
        "extensions": [
          ".sc",
          ".scir"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scribe",
        "scopeName": "source.scribe",
        "path": "./syntaxes/scribe.tmLanguage.json"
      }
    ]
  }
}